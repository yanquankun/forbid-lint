{
  "name": "forbid-lint",
  "version": "1.0.0",
  "description": "一个用于前端工程中禁止修改lint文件的插件",
  "main": "dist/index.js",
  "bin": {
    "forbid-lint": "./dist/index.js"
  },
  "scripts": {
    "eslint": "eslint src --ext .ts",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc && gulp",
    "gulp": "gulp",
    "test": "sh test-install.sh && gulp && tsc -w && nodemon --watch src --ext ts"
  },
  "files": [
    "dist",
    "README",
    "CHANGELOG",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanquankun/forbid-lint"
  },
  "bugs": {
    "url": "https://github.com/yanquankun/forbid-lint/issues"
  },
  "homepage": "https://github.com/yanquankun/forbid-lint/blob/master/README.md",
  "author": "yanquankun",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lint",
    "fe",
    "forbid"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/gulp-copy": "^0.0.7",
    "@types/gulp-terser": "^1.2.6",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.57.1",
    "gulp": "^5.0.0",
    "gulp-copy": "^5.0.0",
    "gulp-terser": "^2.1.0",
    "nodemon": "^3.1.9",
    "parser^6.4.1": "link:@typescript-eslint/parser^6.4.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "7.1.0",
    "core-js": "^3.40.0",
    "ejs": "^3.1.10",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "promise.withresolvers": "^1.0.3",
    "strip-ansi": "6",
    "tslib": "^2.8.1"
  }
}
